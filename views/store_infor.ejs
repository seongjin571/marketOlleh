    <script>

        var reset_stamp_count = 0;
        var stamp_standard = '<%= stamp_standard %>';
        console.log(stamp_standard);
        if (stamp_standard == 5) {
            $('.fiveControls_sj').css('display', 'table');

        }

        if (stamp_standard == 10) {
            $('.tenControls_sj').css('display', 'table');

        }
        $('.button_sj').click(function(){
          var market_name = '<%= market_name %>';
          var sijang_name = '<%= sijang_name %>';
          var user_id = '<%= user_id %>';
          var stamp_count = 0;
          var stamp_standard = '<%= stamp_standard %>';
          var stamp_reward = '<%= stamp_reward %>';
          var stamp_password = '<%= stamp_password %>';
          var data = {
            'market_name' : market_name,
            'sijang_name' : sijang_name,
            'user_id' : user_id,
            'stamp_count' : stamp_count,
            'stamp_standard' : stamp_standard,
            'stamp_reward' : stamp_reward,
            'stamp_password' : stamp_password,
          }
          $.ajax({ // ajax 통신으로 지원자 입력한 정보를 서버에 보낸다.
            type: 'POST',
            url: '/make_stamp',
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            cache: false,
            dataType: 'json',
            data: data,
            success: function (result) {
              if (result['result'] == 'success') {
                alert('스탬프생성');
                location.reload();
              } else if(result['result']=='already'){
                alert('스탬프 이미 존재');
                location.reload();
              }
            },
            error: function (error) {
              console.log('erer');
            }
          });
        });
    </script>
